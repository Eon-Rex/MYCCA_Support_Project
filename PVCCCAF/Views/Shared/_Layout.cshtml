                    <li>@Html.ActionLink("Catalog", "Catalog", "Home")</li>
                    <li>@Html.ActionLink("Reset Password", "ResetPassword", "Authentication")</li>
                    <li>@Html.ActionLink("Manage Catalog", "ManageCatalog", "Home")</li>
                    <li>@Html.ActionLink("Manage Notification", "ManageNotification", "Home")</li>
                    <li>@Html.ActionLink("Manage Hierarchy", "ManageHierarchy", "Home")</li>
