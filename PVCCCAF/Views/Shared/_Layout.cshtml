                    DataTable menulist = (DataTable)Session["MENULIST"];
                    foreach (DataRow _row in menulist.Rows)
                    {
                        if (_row["ROLECODE"].Equals(Session["ROLECODE"].ToString()) && (_row["CONTROLCODE"].Equals("AC01")) && _row["DATAAREAID"].Equals(Session["DATAAREAID"].ToString()))
                        {
                            <li>@Html.ActionLink(_row["LINKTEXT"].ToString(), _row["ACTION"].ToString(), _row["CONTROLER"].ToString())</li>
                        }
                    }
                }
                @*<li><a href="../Home/EmailConfiguration">New Catalog</a></li>*@
                @*<li>@Html.ActionLink("Home", "Dashboard","Home")</li>
                    <li>@Html.ActionLink("My Profile", "MyProfile", "Home")</li>
