          @*<button class="cancel-demo cancelAction">Cancel</button>*@
          <button class="cancel-demo doAction">OK</button>
        </div>
      </div>
    </div>
    <div class="DraftOrderPopUp">
      <div class="demo-second">

        <h2>Alert</h2>
        <p><span id="lblMessage"></span></p>
